# Environment Configuration
NODE_ENV=development

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_API_TIMEOUT=30000
EXPO_PUBLIC_MAX_RETRIES=3

# Security
EXPO_PUBLIC_ENABLE_SSL_PINNING=false
EXPO_PUBLIC_REQUIRE_BIOMETRIC=false

# Features
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false
EXPO_PUBLIC_OFFLINE_MODE=true

# Performance
EXPO_PUBLIC_IMAGE_CACHE_SIZE=100
EXPO_PUBLIC_MAX_CONCURRENT_REQUESTS=5

# Debug
EXPO_PUBLIC_ENABLE_DEBUG_LOGS=true
EXPO_PUBLIC_LOG_LEVEL=debug
